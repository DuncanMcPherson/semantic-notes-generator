<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageId>SemanticRelease.NotesGenerator</PackageId>
        <Authors>Duncan McPherson</Authors>
        <PackageProjectUrl>https://github.com/DuncanMcPherson/semantic-commit-analyzer</PackageProjectUrl>
        <RepositoryUrl>https://github.com/DuncanMcPherson/semantic-commit-analyzer</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
    </PropertyGroup>
    <ItemGroup>
        <None Include="LICENSE" Pack="true" PackagePath="" />
        <None Include="README.md" Pack="true" PackagePath="" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="LibGit2Sharp" Version="0.31.0" />
        <PackageReference Include="SemanticRelease.Abstractions" Version="1.1.0" />
    </ItemGroup>
</Project>
